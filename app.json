{
  "expo": {
    "name": "Cend Rider",
    "slug": "cend-rider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/cend-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#8020A2"
    },
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#8020A2",
          "image": "./assets/cend-logo.png",
          "imageWidth": 200
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8020A2"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
